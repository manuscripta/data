   <define name="teiHeader">
      <element name="teiHeader">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(TEI header) supplies descriptive and declarative metadata associated with a digital resource or set of resources. [2.1.1. The TEI Header and Its Components 15.1. Varieties of Composite Text]</a:documentation>
         <ref name="fileDesc"/>
         <ref name="revisionDesc"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="manuscripta-org-teiHeader-greek.check-constraint-rule-15">
            <sch:rule xmlns:xi="http://www.w3.org/2001/XInclude"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="//*[not(@xml:lang='grc' or @xml:lang='el')]">
                                    <sch:report test="text()[matches(., '\p{IsGreek}|\p{IsGreekExtended}')]"> Use the xml:lang="grc" for Ancient Greek text and xml:lang="el" for Modern Greek text </sch:report>
                                </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="manuscripta-org-teiHeader-latin.check-constraint-rule-16">
            <sch:rule xmlns:xi="http://www.w3.org/2001/XInclude"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="//*[@xml:lang='grc' or @xml:lang='el']">
                                    <sch:report test="text()[matches(., '[(\p{N}|\p{L})-[\p{IsGreek}\p{IsGreekExtended}]]')]"> Only characters from Greek and Greek extended chart range are allowed in elements with the xml:lang="grc" or xml:lang="el".</sch:report>
                                </sch:rule>
         </pattern>
         <sch:pattern xmlns:xi="http://www.w3.org/2001/XInclude"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron">
                                    <sch:rule context="//*[(@xml:lang='grc' or @xml:lang='el')]">
                                        <sch:report test="contains(., '')"> Use Greek small letter stigma (U+03DB).</sch:report>
                                        <sch:report test="contains(., '')"> Use Greek small letter koppa (U+03DF).</sch:report>
                                        <sch:report test="contains(., '')"> Use Greek small letter sampi (U+03E1).</sch:report>
                                        <sch:report test="contains(., '')"> Use the semicolon (U+003B) for the Greek question mark.</sch:report>
                                        <sch:report test="contains(., '')"> Use the middle dot (U+00B7) for the Greek semicolon.</sch:report>
                                        <sch:report test="contains(., &#34;'&#34;)"> Use the right single quotation mark (U+2019) for the Greek apostrophe.</sch:report>
                                        <sch:report test="contains(., '')"> Use Greek small letter alpha with tonos (U+03AC).</sch:report>
                                        <sch:report test="contains(., '')"> Use Greek small letter epsilon with tonos (U+03AD).</sch:report>
                                        <sch:report test="contains(., '')"> Use Greek small letter eta with tonos (U+03AE).</sch:report>
                                        <sch:report test="contains(., '')"> Use Greek small letter iota with tonos (U+03AF).</sch:report>
                                        <sch:report test="contains(., '')"> Use Greek small letter omicron with tonos (U+03CC).</sch:report>
                                        <sch:report test="contains(., '')"> Use Greek small letter upsilon with tonos (U+03CD).</sch:report>
                                        <sch:report test="contains(., '')"> Use Greek small letter omega with tonos (U+03CE).</sch:report>
                                        <sch:report test="contains(., '')"> Use Greek small letter iota with dialytika and tonos (U+0390).</sch:report>
                                        <sch:report test="contains(., '')"> Use Greek small letter upsilon with dialytika and tonos (U+03B0).</sch:report>
                                    </sch:rule>
                                </sch:pattern>
         <sch:pattern xmlns:xi="http://www.w3.org/2001/XInclude"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron">
                                    <sch:rule context="//*[not(@xml:lang='grc' or @xml:lang='el')]">
                                        <sch:assert test="not(contains(., &#34;'&#34;))"> Use the right single quotation mark (U+2019) for apostrophe.</sch:assert>
                                    </sch:rule>
                                </sch:pattern>
         <sch:pattern xmlns:xi="http://www.w3.org/2001/XInclude"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron">
                                    <sch:rule context="text()">
                                        <sch:assert test="not(contains(., '&#34;'))"> The use of quotation marks is not allowed. Use the q element instead.</sch:assert>
                                    </sch:rule>
                                </sch:pattern>
         <sch:pattern xmlns:xi="http://www.w3.org/2001/XInclude"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron">
                                    <sch:rule context="text()">
                                        <sch:assert test="not(contains(., ''))"> The use of quotation marks is not allowed. Use the q element instead.</sch:assert>
                                    </sch:rule>
                                </sch:pattern>
         <attribute name="xml:lang">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(language) indicates the language of the element content using a tag generated according to BCP 47.</a:documentation>
            <choice>
               <value>en</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>sv</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            </choice>
         </attribute>
         <empty/>
      </element>
   </define>
