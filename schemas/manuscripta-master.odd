                                <sch:rule context="//tei:locusGrp">
                                    <sch:report test="count(tei:locus) &lt; 2"> Only use locusGrp when grouping together more than one locus.</sch:report>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="locus.check" scheme="schematron">
                            <desc versionDate="2014-11-17">Check for prohibited values in the from and to attributes</desc>
                            <constraint>
                                <sch:rule context="tei:locus">
                                    <sch:report test="@from[starts-with(., 'i') or starts-with(., 'v') or starts-with(., 'x')] or @to[starts-with(., 'i') or starts-with(., 'v') or starts-with(., 'x')]"> The use of roman numerals is not allowed. Use L1r or L1v for left endleaves and R1r or R1v for right endleaves.</sch:report>
                                    <sch:report test="@from[matches(., 'lco')] or @to[matches(., 'lco')]"> Use LCO for left cover outside.</sch:report>
                                    <sch:report test="@from[matches(., 'lci')] or @to[matches(., 'lci')]"> Use LCI for left cover inside.</sch:report>
                                    <sch:report test="@from[matches(., 'rci')] or @to[matches(., 'rci')]"> Use RCI for right cover inside.</sch:report>
                                    <sch:report test="@from[matches(., 'rco')] or @to[matches(., 'rco')]"> Use RCO for right cover outside.</sch:report>
                                    <sch:report test="@from[matches(., 'Spine')] or @to[matches(., 'Spine')]"> Use spine with lower-case.</sch:report>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="locus.scheme.pages" scheme="schematron">
                            <desc versionDate="2014-11-17">Check that the <att>scheme</att> attribute has the value <val>folios</val> when referring to folios.</desc>
                            <constraint>
                                <sch:rule context="tei:locus[@scheme='pages' or @scheme='other']">
                                    <sch:report test="@from[contains(., 'r') or contains(., 'v')]"> Use the scheme attribute with the value folios when referring to folios.</sch:report>
                                    <sch:report test="@to[contains(., 'r') or contains(., 'v')]"> Use the scheme attribute with the value folios when referring to folios.</sch:report>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="locus.scheme.other" scheme="schematron">
                            <desc versionDate="2014-11-17">Check that the <att>scheme</att> attribute has the value <val>other</val> when referring to the spine och covers.</desc>
                            <constraint>
                                <sch:rule context="tei:locus[@scheme='folios' or @scheme='pages']">
                                    <sch:report test="@from[matches(., 'spine')] or @from[matches(., 'LCO')] or @from[matches(., 'LCI')] or @to[matches(., 'spine')] or @to[matches(., 'RCO')] or @to[matches(., 'RCI')]"> Use the scheme attribute with the value other when referring to the spine or covers.</sch:report>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                        <attList>
                            <attDef ident="from" mode="change" usage="req"/>
                            <attDef ident="to" mode="change" usage="req"/>
                            <attDef ident="scheme" mode="change" usage="req">
                                <valList mode="replace" type="closed">
                                    <valItem ident="folios"/>
                                    <valItem ident="pages"/>
                                    <valItem ident="other">
                                        <desc>Use for spine and covers</desc>
                                    </valItem>
                                </valList>
                            </attDef>
                        </attList>
                        <exemplum>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <locus from="1r" to="119r" scheme="folios"/>
                            </egXML>
                        </exemplum>
                    </elementSpec>

                    <elementSpec ident="locusGrp" mode="change" module="msdescription">
                        <classes mode="change">
                            <memberOf key="att.global" mode="delete"/>
                        </classes>
                        <attList>
                            <attDef ident="scheme" mode="delete"/>
                        </attList>
                        <exemplum>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <locusGrp>
                                    <locus from="24r" to="29v" scheme="folios"/>
                                    <locus from="32r" to="32v" scheme="folios"/>
                                    <locus from="31r" to="31v" scheme="folios"/>
