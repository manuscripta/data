                    <elementSpec ident="msContents" mode="change" module="msdescription">
                        <classes mode="change">
                            <memberOf key="att.global" mode="delete"/>
                            <memberOf key="att.msExcerpt" mode="delete"/>
                        </classes>
                        <content>
                            <elementRef key="summary" minOccurs="0" maxOccurs="1"/>
