                    <elementSpec ident="measure" mode="change" module="core">
                        <classes mode="change">
                            <memberOf key="att.global" mode="delete"/>
                        </classes>
                        <content>
                            <textNode/>
                        </content>
                        <constraintSpec ident="measure.folding" scheme="schematron">
                            <desc versionDate="2014-11-19">Check that <gi>measure</gi>
                                <att>type</att>
                                <val>folding</val> only contains the permitted values "Folio", "Quarto", or "Octavo".</desc>
                            <constraint>
                                <sch:rule context="//tei:measure[@type='folding']">
                                    <sch:assert test="text()='Folio' or text()='Quarto' or text()='Octavo'"> Permitted values are "Folio", "Quarto", or "Octavo".</sch:assert>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                        <attList>
                            <attDef ident="type" mode="change" usage="req">
                                <valList mode="replace" type="closed">
                                    <valItem ident="leftFlyleaves"/>
                                    <valItem ident="rightFlyleaves"/>
                                    <valItem ident="textblockLeaves"/>
                                    <valItem ident="laidLinesIn20mm">
                                        <desc>Number of laid lines in 20 mm</desc>
                                    </valItem>
                                    <valItem ident="folding">
                                        <desc>The folding of the sheets: Folio, Quarto, Octavo</desc>
                                    </valItem>
                                </valList>
                            </attDef>
                            <!-- Attributes changed in att.measurement -->
                            <attDef ident="unit" mode="change" usage="rec"/>
                            <attDef ident="quantity" mode="change" usage="rec"/>
                        </attList>
                        <exemplum>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <extent>
                                    <measure type="leftFlyleaves" quantity="0" unit="folios"/>
                                    <measure type="rightFlyleaves" quantity="2" unit="folios"/>
                                </extent>
                            </egXML>
                        </exemplum>
                        <exemplum>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <measure type="folding">Quarto</measure>
                            </egXML>
                        </exemplum>
