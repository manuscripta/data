                                        <locus from="27v:12" to="27v:31" scheme="folios"/>
                                        <author>
                                            <persName ref="#ManHol"/>
                                        </author>
                                        <title ref="http://pinakes.irht.cnrs.fr/notices/oeuvre/id/1420" type="uniform" xml:lang="la">Commentarius in securim</title>
                                        <rubric xml:lang="grc">Τοῦ Ὁλοβόλου εἰς τὸν Πέλεκυν ἐξήγησις συνοπτική</rubric>
                                        <incipit xml:lang="grc" defective="false">Ὁ πέλεκυς οὐχ <supplied reason="omitted" xml:lang="grc">ὡς</supplied> ὁ Σύριγξ καὶ οἱ Βωμοὶ</incipit>
                                        <explicit xml:lang="grc" defective="false">ὄλβιος καὶ εὐτυχὴς ἀεὶ πνεῖ καὶ ζῇ</explicit>
                                    </msItem>
                                </msContents>
                            </egXML>
                        </exemplum>
                    </elementSpec>

                    <elementSpec ident="msDesc" mode="change" module="msdescription">
                        <classes mode="change">
                            <memberOf key="att.typed" mode="delete"/>
                            <memberOf key="att.docStatus" mode="delete"/>
                        </classes>
                        <content>
                            <elementRef key="msIdentifier" minOccurs="1" maxOccurs="1"/>
                            <elementRef key="model.headLike" minOccurs="1" maxOccurs="1"/>
                            <elementRef key="msContents" minOccurs="0" maxOccurs="1"/>
                            <elementRef key="physDesc" minOccurs="0" maxOccurs="1"/>
                            <elementRef key="history" minOccurs="0" maxOccurs="1"/>
                            <elementRef key="additional" minOccurs="0" maxOccurs="1"/>
                            <elementRef key="msPart" minOccurs="1" maxOccurs="unbounded"/>
                        </content>
                        <constraintSpec ident="folio.ref.check" scheme="schematron">
                            <desc versionDate="2014-09-26" xml:lang="en">Checks that folio and page references are only given in <gi>locus</gi> elements and not in the running text.</desc>
                            <constraint>
                                <sch:pattern>
                                    <sch:rule context="tei:msDesc//*">
                                        <sch:report test="text()[matches(., ' f. \p{N}| ff. \p{N}')]"> Do not use f./ff. in the running text for folio references. Use the locus element instead.</sch:report>
                                        <sch:report test="text()[matches(., ' p. \p{N}| pp. \p{N}')]"> Do not use p./pp. in the running text for page references. Use the locus element instead.</sch:report>
                                    </sch:rule>
                                </sch:pattern>
                            </constraint>
                        </constraintSpec>
                        <attList>
                            <!-- Attributes changed and deleted in att.global -->
                            <attDef ident="xml:lang" mode="change" usage="req"/>
                            <attDef ident="xml:id" mode="delete"/>
                            <attDef ident="n" mode="delete"/>
                            <!-- Attributes deleted in att.global.facs -->
                            <attDef ident="facs" mode="delete"/>
                            <!-- Attributes deleted in att.global.linking -->
                            <attDef ident="sameAs" mode="delete"/>
                            <!-- Attributes deleted in att.global.rendition -->
                            <attDef ident="rend" mode="delete"/>
                            <!-- Attributes changed and deleted in att.global.responsibility -->
                            <attDef ident="resp" mode="change" usage="req"/>
                            <attDef ident="cert" mode="delete"/>
                        </attList>
                        <exemplum>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <msDesc>
                                    <msIdentifier>
                                        <country ref="https://www.manuscripta.se/place/100081">Sweden</country>
                                        <settlement ref="https://www.manuscripta.se/place/100089">Uppsala</settlement>
                                        <institution ref="https://www.manuscripta.se/place/100091">Uppsala University</institution>
                                        <repository ref="https://www.manuscripta.se/place/100092">Uppsala University Library</repository>
                                        <idno type="shelfmark">Gr. 27</idno>
                                    </msIdentifier>
                                    <head>Aelian, On Animals</head>
                                    <!-- MS description -->
                                </msDesc>
                            </egXML>
                        </exemplum>
                    </elementSpec>

                    <elementSpec ident="msIdentifier" module="msdescription" mode="change">
