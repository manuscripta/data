                        </content>
                        <constraintSpec ident="greek.check" scheme="schematron">
                            <desc versionDate="2014-08-01">Check that <att>xml:lang</att> with <val>grc</val> is used for Greek characters</desc>
                            <constraint>
                                <sch:rule context="//*[not(@xml:lang='grc' or @xml:lang='el')]">
                                    <sch:report test="text()[matches(., '\p{IsGreek}|\p{IsGreekExtended}')]"> Use the xml:lang="grc" for Ancient Greek text and xml:lang="el" for Modern Greek text </sch:report>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="latin.check" scheme="schematron">
                            <desc versionDate="2014-08-01">Checks that <att>xml:lang</att> with <val>grc</val> is not used for Latin letters or numbers</desc>
                            <constraint>
                                <sch:rule context="//*[@xml:lang='grc' or @xml:lang='el']">
                                    <sch:report test="text()[matches(., '[(\p{N}|\p{L})-[\p{IsGreek}\p{IsGreekExtended}]]')]"> Only characters from Greek and Greek extended chart range are allowed in elements with the xml:lang="grc" or xml:lang="el".</sch:report>
                                </sch:rule>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="check.forbidden.Greek.charcters" scheme="schematron">
                            <desc versionDate="2014-08-01">Check for forbidden Greek characters</desc>
                            <constraint>
                                <sch:pattern>
                                    <sch:rule context="//*[(@xml:lang='grc' or @xml:lang='el')]">
                                        <sch:report test="contains(., 'Ϛ')"> Use Greek small letter stigma (U+03DB).</sch:report>
                                        <sch:report test="contains(., 'Ϟ')"> Use Greek small letter koppa (U+03DF).</sch:report>
                                        <sch:report test="contains(., 'Ϡ')"> Use Greek small letter sampi (U+03E1).</sch:report>
                                        <sch:report test="contains(., ';')"> Use the semicolon (U+003B) for the Greek question mark.</sch:report>
                                        <sch:report test="contains(., '·')"> Use the middle dot (U+00B7) for the Greek semicolon.</sch:report>
                                        <sch:report test="contains(., &quot;&apos;&quot;)"> Use the right single quotation mark (U+2019) for the Greek apostrophe.</sch:report>
                                        <sch:report test="contains(., 'ά')"> Use Greek small letter alpha with tonos (U+03AC).</sch:report>
                                        <sch:report test="contains(., 'έ')"> Use Greek small letter epsilon with tonos (U+03AD).</sch:report>
                                        <sch:report test="contains(., 'ή')"> Use Greek small letter eta with tonos (U+03AE).</sch:report>
                                        <sch:report test="contains(., 'ί')"> Use Greek small letter iota with tonos (U+03AF).</sch:report>
                                        <sch:report test="contains(., 'ό')"> Use Greek small letter omicron with tonos (U+03CC).</sch:report>
                                        <sch:report test="contains(., 'ύ')"> Use Greek small letter upsilon with tonos (U+03CD).</sch:report>
                                        <sch:report test="contains(., 'ώ')"> Use Greek small letter omega with tonos (U+03CE).</sch:report>
                                        <sch:report test="contains(., 'ΐ')"> Use Greek small letter iota with dialytika and tonos (U+0390).</sch:report>
                                        <sch:report test="contains(., 'ΰ')"> Use Greek small letter upsilon with dialytika and tonos (U+03B0).</sch:report>
                                    </sch:rule>
                                </sch:pattern>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="apostrophe.check" scheme="schematron">
                            <desc versionDate="2014-08-01">Check for the apostrophe U+0027</desc>
                            <constraint>
                                <sch:pattern>
                                    <sch:rule context="//*[not(@xml:lang='grc' or @xml:lang='el')]">
                                        <sch:assert test="not(contains(., &quot;&apos;&quot;))"> Use the right single quotation mark (U+2019) for apostrophe.</sch:assert>
                                    </sch:rule>
                                </sch:pattern>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="quotationmark.check" scheme="schematron">
                            <desc versionDate="2014-08-01">Check for the straight double quotation mark (U+0022)</desc>
                            <constraint>
                                <sch:pattern>
                                    <sch:rule context="text()">
                                        <sch:assert test="not(contains(., '&quot;'))"> The use of quotation marks is not allowed. Use the q element instead.</sch:assert>
                                    </sch:rule>
                                </sch:pattern>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="left.quotationmark.check" scheme="schematron">
                            <desc versionDate="2014-08-01">Check for the left single quotation mark (U+2018)</desc>
                            <constraint>
                                <sch:pattern>
                                    <sch:rule context="text()">
                                        <sch:assert test="not(contains(., '‘'))"> The use of quotation marks is not allowed. Use the q element instead.</sch:assert>
                                    </sch:rule>
                                </sch:pattern>
                            </constraint>
                        </constraintSpec>
