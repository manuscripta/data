               <head>Missal, “Helgeandshusmissal”</head>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc>
                        <support>
                           <p>
                              <locusGrp>
                                 <locus from="LCI" to="LCI" scheme="other"/>
                                 <locus from="SL1r" to="SL1v" scheme="folios"/>
