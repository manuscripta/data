                        </msIdentifier>
                        <msContents>
                            <textLang mainLang="la"/>
                            <msItem>
                                <locus from="Fragment_1r" to="Fragment_1v" scheme="other"/>
                                <title type="desc">Fragment 1 of Passio S. Dionysii</title>
                                <incipit>Passio sancti dionisii et sociorum eius</incipit>
                                <explicit>a pastoribus</explicit>
                                <note>
                                    <p>The recto contains part of a table of content.</p>
                                    <p>The verso is damaged by glue, but contains text from the prose version of the Passio S. Dionysii, parts of chapters 1-3.</p>
                                </note>
                                <bibl>
                                    <title ref="https://www.manuscripta.se/bibl/107333" type="short">Lapidge (2017)</title>
                                    <citedRange unit="page" from="232" to="236"/>
                                </bibl>
                            </msItem>
                            <msItem>
                                <locus from="Fragment_2r" to="Fragment_2v" scheme="other"/>
                                <title type="desc">Fragment 2 of Passio S. Dionysii </title>
                                <incipit>
                                              <supplied reason="damage">fo</supplied>re discip<supplied reason="damage">ulum</supplied> his ita gesti<supplied reason="damage">s</supplied>
                                                                                                              
                                                                                                    
                                                                                          
                                                                                
                                                                      
                                                            
                                                  
                                          
                                          
                                     
                                     
                                </incipit>
                                <explicit>que illa ora qui sunt illi tactus</explicit>
                                <note>
                                    <p>Recto: text from the Passio S. Dionysii, parts of chapters 4-6.</p>
                                    <p>Verso: text from the Passio S. Dionysii, parts of chapters 8-9.</p>
                                </note>
                                <bibl>
                                    <title ref="https://www.manuscripta.se/bibl/107333" type="short">Lapidge (2017)</title>
                                    <citedRange unit="page" from="238" to="242"/>
                                    <citedRange unit="page" from="242" to="246"/>
                                </bibl>
                            </msItem>
                        </msContents>
                        <physDesc>
                            <objectDesc>
                                <supportDesc material="parchment">
                                    <support>
                                        <p>
                                            <locus from="Fragment_1" to="Fragment_1" scheme="other"/>
                                            <material>Parchment</material>
                                            <dimensions type="leaf">
                                                <height quantity="185" unit="mm"/>
                                                <width quantity="275" unit="mm"/>
                                            </dimensions>
                                        </p>
                                        <p>
                                            <locus from="Fragment_2" to="Fragment_2" scheme="folios"/>
                                            <material>Parchment</material>
                                            <dimensions type="leaf">
                                                <height quantity="135" unit="mm"/>
                                                <width quantity="275" unit="mm"/>
                                            </dimensions>
