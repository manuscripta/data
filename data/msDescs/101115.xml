                            <objectDesc>
                                <supportDesc material="paper">
                                    <support>
                                        <p>
                                            <locus from="1r" to="12v" scheme="folios"/>
                                            <watermark n="1">
                                                <term type="watermarkMotif">flower, possibly four-petalled, with strip and not identified additional motifs</term>
                                            </watermark>
                                            <material>Western paper</material>
                                            <dimensions type="leaf">
